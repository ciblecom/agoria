<ul id="archive">
  <% for calendar in @calendars %>
    <li>
<b><%= calendar.name %></b><br>      
<%= image_tag(calendar.month.to_s+'_small.jpg') %>
<p class="actions">
<%= link_to "Exporter Calendrier", calendar_path(calendar, :format => "pdf", :what => 'calendar'), :target => "_blank" %> - 
<%= link_to "Exporter Affiche", calendar_path(calendar, :format => "pdf", :what => 'poster'), :target => "_blank" %><br>
<%= link_to "Modifier", edit_calendar_path(calendar) %>
<hr>
    </li>
  <% end %>
</ul>
<p align="center"><%= link_to "Nouveau calendrier", new_calendar_path, :class => "purple button_submit" %></p>
