<table>
  <tr>
    <th>Name</th>
  </tr>
  <% for calendar in @calendars %>
    <tr>
      <td><%= calendar.name %></td>
      <td><%= link_to "Edit", edit_calendar_path(calendar) %> - <p id="pdf_link"><%= link_to "exporter", calendar_path(calendar, :format => "pdf") %></p></td>
    </tr>
  <% end %>
</table>

<p><%= link_to "New Calendar", new_calendar_path %></p>
